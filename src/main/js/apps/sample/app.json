                ]
            }
        },
        "selection-ui": {
            "Config": {
                "preselectSources": "none",
                "activateSelectionMethod": true,
                "closeOnSelection": false,
                "selectionMethods": [
                    "*"
                ]
            }
        },
        "agssearch": {
            "AGSStore": [
                {
                    "title": "Zensus 2011",
                    "id": "zensus_store",
                    "description": "",
                    "idProperty": "OBJECTID",
                    "url": "http://services.arcgis.com/OLiydejKCZTGhvWg/arcgis/rest/services/Bev%C3%B6lkerungNationalit%C3%A4t1B/FeatureServer/1",
                    "useIn": [
                        "selection"
                    ],
                    "filterOptions": {
                        "suggestContains": true
                    },
                    "fetchIdProperty": true,
                    "enablePagination": false
                },
                {
                    "title": "Kriminalstatistiken 2013",
                    "id": "crime_store",
                    "description": "",
                    "idProperty": "OBJECTID",
                    "url": "https://services1.arcgis.com/wdMHWK8xfn32LjIy/arcgis/rest/services/BKAPKS_landkreise_STinsgesamt/FeatureServer/0",
                    "useIn": [
                        "selection"
                    ],
                    "filterOptions": {
                        "suggestContains": true
                    },
                    "fetchIdProperty": true,
                    "enablePagination": false
                }
            ]
        },
        "toolset": {
            "ToolsetManager": {
                "toolsets": [
                    {
                        "id": "mapview_tools",
                        "tools": [
                            "locateMeTool",
                            "zoomInTool",
                            "zoomOutTool",
                            "compassTool",
                            "restoreInitialViewTool",
                            "viewmodeSwitcherTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "mapview_tools"
                        },
                        "container": "ignore",
                        "windowType": "container",
                        "cssClass": "muted"
                    },
                    {
                        "id": "tools",
                        "title": "Floating Toolbar with Tool Labels",
                        "tools": [
                            "selection-ui-tool"
                        ],
                        "container": "map",
                        "position": {
                            "rel_l": 20,
                            "rel_t": 20
                        },
                        "cssClass": "showToolLabels",
                        "windowType": "container"
                    }
                ]
            }
        },
        "basemaptoggler": {
            "Config": {
                "nextBasemap": "hybrid"
            }
        },
        "map-init": {
            "Config": {
                "map": {
                    "basemap": "streets-night-vector",
                    "layers": [
                        {
                            "id": "zensus",
                            "title": "Zensus 2011",
                            "url": "http://services.arcgis.com/OLiydejKCZTGhvWg/arcgis/rest/services/Bev%C3%B6lkerungNationalit%C3%A4t1B/FeatureServer/1",
                            "type": "AGS_FEATURE",
                            "elevationInfo": {
                                "mode": "on-the-ground"
                            },
                            "visible": true,
                            "outFields": [
                                "*"
                            ]
                        }
                    ],
                    "ground": {
                        "url": "https://elevation3d.arcgis.com/arcgis/rest/services/WorldElevation3D/Terrain3D/ImageServer",
                        "type": "ELEVATION"
                    }
                },
                "view": {
                    "viewmode": "2D",
                    "scale": 5000000,
                    "center": {
                        "x": 1159578.07,
                        "y": 6644912.76,
                        "spatialReference": {
                            "wkid": 3857
                        }
                    }
                }
            }
        },
        "themes": {
            "ThemeModel": {
                "_selectedTheme": "everlasting"
            },
            "ThemeSelector": {
                "componentEnabled": true
            }
        }
    }
}
