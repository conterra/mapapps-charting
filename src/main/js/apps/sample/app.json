                                    {
                                        "attribute": "ALTER_5",
                                        "title": "65 Jahre und älter"
                                    }
                                ],
                                "dataOrientation": "rows",
                                "showDataLabels": false,
                                "rotatedAxis": false,
                                "expanded": true
                            },
                            {
                                "title": "Geschlechterverteilung",
                                "type": "pie",
                                "height": 400,
                                "data": [
                                    {
                                        "attribute": "EW_M",
                                        "title": "Männlich"
                                    },
                                    {
                                        "attribute": "EW_W",
                                        "title": "Weiblich"
                                    }
                                ],
                                "dataOrientation": "rows",
                                "showDataLabels": false,
                                "rotatedAxis": false,
                                "expanded": true
                            }
                        ]
                    },
                    {
                        "storeId": "crime_store",
                        "titleAttribute": "Landkreis",
                        "charts": [
                            {
                                "title": "Kriminalfälle",
                                "type": "bar",
                                "height": 400,
                                "data": [
                                    {
                                        "attribute": "faelle",
                                        "title": "Fälle insgesamt"
                                    },
                                    {
                                        "attribute": "F1SW_drohung",
                                        "title": "Drohung mit Schusswaffe"
                                    },
                                    {
                                        "attribute": "SW_geschossen",
                                        "title": "Schusswaffe gefeuert"
                                    },
                                    {
                                        "attribute": "faelle_gekl",
                                        "title": "Fälle geklärt"
                                    },
                                    {
                                        "attribute": "ungfaelle",
                                        "title": "Fälle ungeklärt"
