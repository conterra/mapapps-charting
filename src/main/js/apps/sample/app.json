{
    "properties": {
        "title": "${apptitle}"
    },
    "load": {
        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "allowedBundles": [
            "agssearch",
            "banner",
            "basemaptoggler",
            "languagetoggler",
            "locateme",
            "locator-store",
            "map-init",
            "map-preload-2D",
            "mapnavigation",
            "measurement-2d",
            "notifier",
            "parameter-url",
            "popups-default",
            "printing",
            "result-ui",
            "scalebar",
            "search-ui",
            "selection-ui",
            "sketching",
            "splashscreen",
            "system",
            "template-seasons",
            "templatelayout",
            "theme-autumn",
            "toc",
            "toolset",
            "dn_charting",
            "dn_imprintprivacy",
            "dn_querybuilder"
        ],
        "styles": [
            "${app}:app.css"
        ],
        "i18n": [
            "bundle"
        ]
    },
    "bundles": {
        "dn_charting": {
            "ResultCenterChartingTool": {
                "visibility": true
            },
            "ChartingDashboardWidgetModel": {
                "drawTabGeometries": true,
                "drawChartsForSelectionResults": true,
                "relationships": [],
                "chartsProperties": [
                    {
                        "storeId": "Einkommen_Haushalte_Kreise",
                        "titleAttribute": "NAME",
                        "charts": [
                            {
                                "title": "Einkommensverteilung",
                                "type": "bar",
                                "height": 600,
                                "data": [
                                    {
                                        "attribute": "a0",
                                        "title": "verfüg. Einkommen der priv. Haushalte (Tsd. EUR)"
                                    },
                                    {
                                        "attribute": "a1",
                                        "title": "verfüg. Einkommen der priv. Haushalte je Einwohner (EUR)"
                                    }
                                ],
                                "calculationType": "mean",
                                "dataOrientation": "rows",
                                "showDataLabels": true,
                                "rotatedAxis": false,
                                "expanded": true
                            }
                        ]
                    }
                ],
                "chartsTabs": []
            }
        },
        "dn_querybuilder": {
            "QueryBuilderProperties": {
                "useMemorySelectionStore": false
            },
            "QueryTools": []
        },
        "agssearch": {
            "AutoStoreRegistration": {
                "componentEnabled": true
            }
        },
        "banner": {
            "BannerWidget": {
                "label": "Developer Network",
                "image": "resource('${app}:/assets/logo.svg')",
                "imageWidth": 150,
                "imagePosition": "above",
                "link": "http://conterra.github.io"
            }
        },
        "basemaptoggler": {
            "Config": {
                "itemStyles": {
                    "gray": "icon-thumbnail",
                    "streets": "icon-thumbnail",
                    "satellite": "icon-thumbnail",
                    "hybrid": "icon-thumbnail"
                }
            }
        },
        "map-init": {
            "Config": {
                "basemaps": [
                    {
                        "id": "gray",
                        "basemap": "gray-vector",
                        "title": "${map.basemaps.gray}",
                        "thumbnailUrl": "resource('${app}:/assets/gray.png')",
                        "selected": true
                    },
                    {
                        "id": "streets",
                        "basemap": "streets-vector",
                        "title": "${map.basemaps.streets}",
                        "thumbnailUrl": "resource('${app}:/assets/streets.png')"
                    },
                    {
                        "id": "satellite",
                        "basemap": "satellite",
                        "title": "${map.basemaps.topo}",
                        "thumbnailUrl": "resource('${app}:/assets/topo.png')"
                    },
                    {
                        "id": "hybrid",
                        "basemap": "hybrid",
                        "title": "${map.basemaps.hybrid}",
                        "thumbnailUrl": "resource('${app}:/assets/hybrid.png')"
                    }
                ],
                "map": {
                    "layers": [
                        {
                            "id": "Einkommen_Haushalte_Kreise",
                            "title": "Einkommen Haushalte Kreise",
                            "type":"AGS_FEATURE",
                            "url": "https://services2.arcgis.com/jUpNdisbWqRpMo35/ArcGIS/rest/services/a7103e/FeatureServer/0",
                            "visible": true
                        }
                    ]
                },
                "view": {
                    "viewmode": "2D",
                    "center": {
                        "x": 774853,
                        "y": 6610677,
                        "spatialReference": 3857
                    },
                    "scale": 140000
                }
            }
        },
        "toc": {
            "Config": {
                "showBasemaps": true,
                "showLayerLegend": true
            }
        },
        "toolset": {
            "ToolsetManager": {
                "toolsets": [
                    {
                        "id": "mapview_tools",
                        "tools": [
                            "selection-ui-tool",
                            "locateMeTool",
                            "zoomInTool",
                            "zoomOutTool",
                            "compassTool",
                            "restoreInitialViewTool",
                            "viewmodeSwitcherTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "mapview_tools"
                        },
                        "container": "ignore",
                        "windowType": "container",
                        "cssClass": "muted",
                        "tooltipPositions": [
                            "before",
                            "above",
                            "below",
                            "after"
                        ]
                    },
                    {
                        "id": "drawer_left",
                        "title": "${tools.drawerLeft}",
                        "cssClass": "ct-main-app-menu",
                        "tools": [
                            "selection-ui-tool",
                            "sketchToggleTool",
                            "measuring_tools",
                            "printingToggleTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "drawer_button"
                        },
                        "container": "ignore",
                        "windowType": "drawer_left",
                        "tooltipPositions": [
                            "after",
                            "above",
                            "below",
                            "before"
                        ]
                    },
                    {
                        "id": "measuring_tools",
                        "title": "${tools.measuring}",
                        "iconClass": "icon-editor-table",
                        "tools": [
                            "distanceMeasurement2DToggleTool",
                            "areaMeasurement2DToggleTool"
                        ],
                        "container": "ignore",
                        "registerTool": true,
                        "windowType": "menuitem",
                        "tooltipPositions": [
                            "after"
                        ]
                    }
                ]
            }
        },
        "dn_imprintprivacy": {
            "ToolHandler": {
                "imprintUrls": [
                    {
                        "locale": "de",
                        "url": "https://www.conterra.de/impressum"
                    },
                    {
                        "locale": "en",
                        "url": "https://www.con-terra.com/imprint"
                    }
                ],
                "privacyUrls": [
                    {
                        "locale": "de",
                        "url": "https://www.conterra.de/datenschutzhinweise"
                    },
                    {
                        "locale": "en",
                        "url": "https://www.con-terra.com/privacy-statements"
                    }
                ],
                "toolsetDefinition": {
                    "id": "imprintprivacyToolset",
                    "tools": [
                        "imprintTool",
                        "privacyStatementsTool"
                    ],
                    "registerWidget": {
                        "widgetRole": "footer_right"
                    },
                    "container": "ignore",
                    "windowType": "container",
                    "cssClass": "showToolLabels"
                }
            }
        }
    }
}
