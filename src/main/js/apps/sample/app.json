{
    "properties": {
        "title": "${apptitle}"
    },
    "load": {
        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "allowedBundles": [
            "system",
            "console",
            "notifier",
            "splashscreen",
            "templatelayout",
            "template-seasons",
            "theme-everlasting",
            "theme-winter",
            "theme-spring",
            "theme-summer",
            "theme-autumn",
            "map-init",
            "toolset",
            "locateme",
            "viewmodeswitcher",
            "mapnavigation",
            "agssearch",
            "selection-ui",
            "dn_charting"
        ],
        "require": [
            "${app}.app"
        ],
        "i18n": [
            "bundle"
        ]
    },
    "bundles": {
        "dn_charting": {
            "ChartingDashboardWidgetModel": {
                "chartsProperties": [
                    {
                        "storeId": "zensus_store",
                        "titleAttribute": "NAME",
                        "charts": [
