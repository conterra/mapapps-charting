{
    "properties": {
        "title": "${apptitle}"
    },
    "load": {
        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "allowedBundles": [
            "system",
            "console",
            "notifier",
            "splashscreen",
            "templatelayout",
            "template-seasons",
            "theme-everlasting",
            "theme-winter",
            "theme-spring",
            "theme-summer",
            "theme-autumn",
            "map-init",
            "toolset",
            "agssearch",
            "dn_charting"
        ],
        "require": [
            "${app}.app"
        ],
        "i18n": [
            "bundle"
        ]
    },
    "bundles": {
        "dn_charting": {
            "ChartingDashboardWidgetModel": {
                "storeId": "crime_stas",
                "titleAttribute": "Landkreis",
                "initialPoint": {
                    "x": 7.628694,
                    "y": 51.962944
                },
                "charts": [
                    {
                        "title": "Kriminalfälle",
                        "type": "step",
                        "height": 200,
                        "data": [
                            {
                                "attribute": "faelle",
                                "title": "Fälle insgesamt"
                            },
                            {
                                "attribute": "F1SW_drohung",
                                "title": "Drohung mir Schusswaffe"
                            },
                            {
                                "attribute": "SW_geschossen",
                                "title": "Schusswaffe gefeuert"
                            },
                            {
                                "attribute": "faelle_gekl",
                                "title": "Fälle geklärt"
                            },
                            {
                                "attribute": "ungfaelle",
                                "title": "Fälle ungeklärt"
                            }
                        ],
                        "dataOrientation": "rows",
                        "showDataLabels": true,
                        "rotatedAxis": false,
                        "expanded": true
                    },
                    {
                        "title": "Tatverdächtige",
                        "type": "donut",
                        "height": 200,
                        "data": [
                            {
                                "attribute": "TV_m",
                                "title": "Tatverdächtige männlich"
                            },
                            {
                                "attribute": "TV_w",
                                "title": "Tatverdächtige weiblich"
                            }
                        ],
                        "dataOrientation": "rows",
                        "showDataLabels": false,
                        "rotatedAxis": false,
                        "expanded": true
                    },
                    {
                        "title": "Bevölkerung",
                        "type": "bar",
                        "height": 200,
                        "data": [
                            {
                                "attribute": "wanderungssaldo_je10000",
                                "title": "Wanderungssaldo je 10.000"
                            },
                            {
                                "attribute": "gest_je10000",
                                "title": "gestorben je 10.000"
                            },
                            {
                                "attribute": "lebendgeb_je10000",
                                "title": "Geburten je 10.000"
                            }
                        ],
                        "dataOrientation": "rows",
                        "showDataLabels": false,
                        "rotatedAxis": false,
                        "expanded": true
                    }
                ]
            }
        },
        "agssearch": {
            "AGSStore": [
                {
                    "title": "Crime Statistics",
                    "id": "crime_stas",
                    "description": "",
                    "omniSearchLabelAttr": "Landkreis",
                    "omniSearchSearchAttr": "Landkreis",
                    "idProperty": "OBJECTID",
                    "url": "https://services1.arcgis.com/wdMHWK8xfn32LjIy/arcgis/rest/services/BKAPKS_landkreise_STinsgesamt/FeatureServer/0",
                    "useIn": [
                        "selection"
                    ],
                    "filterOptions": {
                        "suggestContains": true
                    },
                    "omniSearchTypingDelay": 500,
                    "omniSearchPageSize": 20,
                    "omniSearchDefaultLabel": "Suchbegriff eingeben...",
                    "fetchIdProperty": true,
                    "enablePagination": false
                }
            ]
        },
        "basemaptoggler": {
            "Config": {
                "nextBasemap": "hybrid"
            }
        },
        "map-init": {
            "Config": {
                "map": {
                    "basemap": "streets-night-vector",
                    "layers": [
                        {
                            "id": "crime_stas",
                            "title": "Kriminalstatistiken",
                            "url": "https://services1.arcgis.com/wdMHWK8xfn32LjIy/arcgis/rest/services/BKAPKS_landkreise_STinsgesamt/FeatureServer/0",
                            "type": "AGS_FEATURE",
                            "elevationInfo": {
                                "mode": "on-the-ground"
                            },
                            "visible": true,
                            "outFields": [
                                "*"
                            ]
                        }
                    ],
                    "ground": {
                        "url": "https://elevation3d.arcgis.com/arcgis/rest/services/WorldElevation3D/Terrain3D/ImageServer",
                        "type": "ELEVATION"
                    }
                },
                "view": {
                    "viewmode": "2D",
                    "scale": 5000000,
                    "center": {
                        "x": 1159578.07,
                        "y": 6644912.76,
                        "spatialReference": {
                            "wkid": 3857
                        }
                    }
                }
            }
        },
        "themes": {
            "ThemeModel": {
                "_selectedTheme": "everlasting"
            },
            "ThemeSelector": {
                "componentEnabled": true
            }
        }
    }
}
